<div class="container">
  <h1>Service Request</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Title</th>
        <th scope="col">Status</th>
        <th scope="col">Type</th>
      </tr>
    </thead>
    <tbody>
      {{#each sr}}
      <tr>
        <th scope="row">{{this.id}}</th>
        <td>{{this.title}}</td>
        <td>{{this.status}}</td>
        <td>{{this.type}}</td>
        <td><a href="/update-sr/{{this.id}}">Edit</a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>